<div class="row-fluid">
  <div class="span12">
  	<div class="widget-box light-border">
  		<div class="widget-header header-color-dark">
  			<h6>DOCUMENTS</h6>

  			<div class="widget-toolbar">
  				<a href="#" data-action="collapse">
  					<i class="icon-chevron-up"></i>
  				</a>
  				<a href="#" data-action="close">
  					<i class="icon-remove"></i>
  				</a>
  			</div>
  		</div>

  		<div class="widget-body">
  			<div class="widget-main">
          <dl>
            <% if @examiner.final_project.document_revision_final_project.present? %>
              <dt>Buku Revisi Tugas Akhir : </dt>
              <dd><%= link_to File.basename(@examiner.final_project.document_revision_final_project.path), @examiner.final_project.document_revision_final_project.url %></dd>
            <% end %>
            <% if @examiner.final_project.document_final_project.present? %>
              <dt>Buku Tugas Akhir : </dt>
              <dd><%= link_to File.basename(@examiner.final_project.document_final_project.path), @examiner.final_project.document_final_project.url %></dd>
            <% end %>
            <dt>Lembar Pemeriksa : </dt>
            <dd><%= link_to File.basename(@examiner.proposal.exam.path), @examiner.proposal.exam.url %></dd>
            <dt>Berita Acara : </dt>
            <dd><%= link_to File.basename(@examiner.proposal.events.path), @examiner.proposal.events.url %></dd>
            <dt>Proposal : </dt>
            <dd><%= link_to File.basename(@examiner.proposal.proposal.path), @examiner.proposal.proposal.url %></dd>
            <dt>SK : </dt>
            <dd><%= link_to File.basename(@examiner.proposal.decree.path), @examiner.proposal.decree.url %></dd>
          </dl>
  			</div>
  		</div>
  	</div>
  </div>
</div>
<%= semantic_form_for @reports, :url => create_report_final_project_admin_final_project_path(@final_project) do |f| %>
  <%= f.inputs :name => "Report Final Project" do %>
    <%= f.input :user, :collection => @final_project.collection_advisor.reject(&:nil?).map{|f| [f.username,f.id]}, :include_blank => false, :label => "Pembimbing" %>
    <%= f.input :note %>
  <% end %>
  <br>
  <%= f.submit "Create Reports" %>
<% end %>